{% extends 'base.html.twig' %}

{% block body %}
    <h1>Emprunter</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ emprunter.id }}</td>
            </tr>
            <tr>
                <th>Datedebut</th>
                <td>{% if emprunter.dateDebut %}{{ emprunter.dateDebut|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{% if emprunter.dateFin %}{{ emprunter.dateFin|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('emprunter_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('emprunter_edit', { 'id': emprunter.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
